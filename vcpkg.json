{
  "name": "tenacity",
  "version": "1.3",
  "description": "Tenacity is an easy-to-use, cross-platform multi-track audio editor/recorder for Windows, MacOS, GNU/Linux and other operating systems.",
  "homepage": "https://tenacityaudio.org/",
  "license": "GPL-2.0-or-later",
  "supports": "!uwp",
  "dependencies": [
    "libsndfile",
    "lilv",
    "libid3tag",
    "libmad",
    "twolame",
    "soxr",
    "soundtouch",
    "libsbsms",
    {
      "name": "portaudio",
      "features": [ "asio" ]
    },
    "portmidi",
    "portsmf",
    "mp3lame",
    "zlib",
    "vamp-sdk",
    "expat",
    "sqlite3",
    "ffmpeg",
    "wxwidgets"
  ]
}
